{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "rl-designer",
  "version": "0.1.0",
  "identifier": "com.alxsm.rldesigner",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:9568",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "rl-designer",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": ["**"]
        }
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo.png",
      "icons/logo.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
  "updater": {
    "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgzM0FDOTlFRUY2M0ZERjkKUldUNS9XUHZuc2s2Zy9rNjJtci9UbC8zRElOYXJXcUg4dUVCK24rSnlJTURZWE9uYXd5Vk1CblMK",
    "endpoints": [
      "https://github.com/ALXS-GitHub/RL-Designer/releases/latest/download/latest.json"
    ]
  }
  }
}
